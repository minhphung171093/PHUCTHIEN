<?xml version="1.0" encoding="utf-8"?>
<openerp>
 <data>
		<!-- Leave Type -->
        
        <record model="ir.ui.view" id="general_edit_holiday_status_form">
            <field name="name">general.hr.holidays.status.form</field>
            <field name="model">hr.holidays.status</field>
            <field name="inherit_id" ref="hr_holidays.edit_holiday_status_form"/>
            <field name="arch" type="xml">
            	<field name="name" position="after">
            		<field name="ratio"/>
            	</field>
            </field>
        </record>
        
        <record model="ir.ui.view" id="general_view_holiday_status_tree">
            <field name="name">general.hr.holidays.status.tree</field>
            <field name="model">hr.holidays.status</field>
            <field name="inherit_id" ref="hr_holidays.view_holiday_status_tree"/>
            <field name="arch" type="xml">
                <field name="name" position="after">
                	<field name="ratio"/>
                </field>
            </field>
        </record>
 </data>  
</openerp>
