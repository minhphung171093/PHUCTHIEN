            <field name="arch" type="xml">
	            <form string="Mission" version='7.0'>
	            	<header>
	            		<button string="To Approve" name="to_approve" states="draft" type="object" class="oe_highlight"/>
                		<button string="To Paid" name="to_paid" states="approve" type="object" class="oe_highlight"/>
