		            	<group>
			                <field name="name"/>
			                <field name="agency_work"/>
			                <field name="rel_id"/>
			                <field name="birthday"/>
			                <field name="nationality_id"/>
		            	</group>
		            	<group>
			                <field name="depend"/>
			                <field name="phone"/>
			                <field name="gender"/>
			                <field name="date_of_death"/>
		                </group>
	                </group>
	                <group>
	                	<field name="career"/>
		                <field name="permanent_address"/>
		                <field name="note"/>
	                </group>
                </sheet>
            </form>            
            </field>
        </record>
        
        <record model="ir.ui.view" id="view_hr_family_tree">
            <field name="name">hr.family.tree</field>
            <field name="model">hr.family</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
	            <tree string="Family">
	                <field name="employee_id"/>
	                <field name="name"/>
	                <field name="birthday"/> 
	                <field name="date_of_death"/>
	                <field name="permanent_address"/>  
	                <field name="gender"/>   
	                <field name="rel_id"/>    
	            </tree>            
            </field>
        </record>
        
        <record id="action_hr_family" model="ir.actions.act_window">
            <field name="name">Family</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hr.family</field>
            <field name="context">{'search_default_active_filter': 1}</field>
            <field name="view_type">form</field>
        </record>
        
        <menuitem id="menu_hr_family"  action="action_hr_family" sequence="4" parent="hr.menu_hr_main"/>
        
<!--Uniform-->   
		<record model="ir.ui.view" id="view_hr_uniform_filter">
