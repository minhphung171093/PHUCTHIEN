	                                                    <div class="oe_module_desc">
	                                                        <field name="name"/>
	                                                        <field name="last_name"/>
	                                                    </div>
	                                                </div>
	                                            </div>
	                                        </t>
	                                    </templates>
	                                </kanban>
	                            </field>
	                        </page>
	                     </notebook>
	                </sheet>
	            </form>            
