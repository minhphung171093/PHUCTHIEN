<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
      
==========================
basic salary company
==========================
		<record id="view_hr_basic_company_filter" model="ir.ui.view">
            <field name="name">view.hr.basic.company.filter</field>
            <field name="model">hr.basic.company</field>
            <field name="type">search</field>
            <field name="arch" type="xml">	
            	<search string="Search Company Basic Salary">
            		<filter icon="terp-personal+" domain="[('active','=',True)]" name="active_filter" string="Active"/>
            		<field name="code"/>
	            	<field name="name"/>     	
	            	<field name="date_start"/>
	            	<field name="date_stop"/>
	            	<field name="active"/>
            	</search>
            </field>
        </record>
        
		<record id="view_hr_basic_company_tree" model="ir.ui.view">
            <field name="name">view.hr.basic.company.tree</field>
            <field name="model">hr.basic.company</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">	
            	<tree string="Company Basic Salary">
            		<field name="code"/>
            		<field name="description"/>
	            	<field name="name"/>
	            	<field name="date_start"/>
	            	<field name="date_stop"/>
	            	<field name="active"/>
            	</tree>
            </field>
        </record>
        
		<record id="view_hr_basic_company_form" model="ir.ui.view">
            <field name="name">view.hr.basic.company.form</field>
            <field name="model">hr.basic.company</field>
            <field name="type">form</field>
            <field name="arch" type="xml">	
            	<form string="Company Basic Salary" version="7.0">
            		<sheet>
            		<div class="oe_title">
	            		<group col="4">
		            		<field name="code"/>
		            		<field name="active"/>
		            		<field name="description" colspan="4"/>
			            	<field name="date_start" class="oe_inline"/> 
				            <field name="date_stop" class="oe_inline"/>
				            <field name="name"/>
		            	</group>
		            </div>	
	            	<div class="oe_right oe_button_box" name="button_box">
                            <!-- Put here related buttons -->
                    </div>
	            	<group col="4">	            	
		            	<separator string="Employees"/>
		            	<field name="employee_ids" colspan="4" nolabel="1"/>
	            	</group>
	            	</sheet>
           		</form>
           	</field>
        </record>           
                 
        <record id="action_hr_basic_company" model="ir.actions.act_window">
            <field name="name">Company Basic Salary</field>
            <field name="res_model">hr.basic.company</field>
            <field name="view_type">form</field>
            <field name="context">{'search_default_active_filter': 1}</field>
            <field name="view_id" ref="view_hr_basic_company_tree"/>
        </record>
        <menuitem id="menu_hr_basic_company" parent="hr_contract.next_id_56" action="action_hr_basic_company"/>

==========================
grade scale
==========================
		<!-- hr.grade -->
		<record id="view_hr_grade_filter" model="ir.ui.view">
           	<field name="name">view.hr.grade.filter</field>
            <field name="model">hr.grade</field>
            <field name="type">search</field>
            <field name="arch" type="xml">	
            	<search string="Grade">
	            	<filter icon="terp-personal+" domain="[('active','=',True)]" name="active_filter" string="Active"/>
	            	<field name="code"/>
	            	<field name="name"/>
	            	<field name="date_start"/>
	            	<field name="date_stop"/>
	            	<field name="active"/>            	
           		</search>
           	</field>
        </record>
        
        <record id="view_hr_grade_tree" model="ir.ui.view">
            <field name="name">view.hr.grade.tree</field>
            <field name="model">hr.grade</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">	
            	<tree string="Grade">
            		<field name="code"/>
	            	<field name="name"/>
	            	<field name="date_start"/>
	            	<field name="date_stop"/>
	            	<field name="active"/>            	
           		</tree>
           	</field>
        </record>
        
		<record id="view_hr_grade_form" model="ir.ui.view">
            <field name="name">view.hr.grade.form</field>
            <field name="model">hr.grade</field>
            <field name="type">form</field>
            <field name="arch" type="xml">	
            <form string="Grade" version="7.0">
            	<sheet>
	            	<group col="6">
		            	<field name="code"/>
		            	<field name="name"/>
		            	<field name="active"/>
		            	<field name="date_start"/>
		            	<field name="date_stop"/>
	            	</group>
	            	<field name="grade_line" nolabel="1" colspan="4">
	            		<tree string="" editable="top">
	            			<field name="code"/>
		            		<field name="name"/>
		            		<field name="ratio" />
		            		<field name="active"/>
	            		</tree>	
	            	</field>
            	</sheet>
            </form>
            </field>
        </record>     
	        
        <record id="action_hr_grade" model="ir.actions.act_window">
            <field name="name">Salary Scale</field>
            <field name="res_model">hr.grade</field>
            <field name="view_type">form</field>
            <field name="context">{'search_default_active_filter': 1}</field>
            <field name="view_id" ref="view_hr_grade_tree"/>
        </record>
        <menuitem id="menu_hr_grade_scale" parent="hr_contract.next_id_56" name="Grade Scale"/>
        <menuitem id="menu_hr_grade" parent="menu_hr_grade_scale" action="action_hr_grade"/>
		
		<!-- hr.grade.line -->	
		<record id="view_hr_grade_line_filter" model="ir.ui.view">
            <field name="name">view.hr.grade.line.filter</field>
            <field name="model">hr.grade.line</field>
            <field name="type">search</field>
            <field name="arch" type="xml">	
            	<search string="Grade">
            		<filter icon="terp-personal+" domain="[('active','=',True)]" name="active_filter" string="Active"/>
	            	<field name="code"/>
	            	<field name="name"/>
	            	<field name="grade_id"/>
	            	<field name="ratio" />
	            	<field name="active"/>	            	              
	                <group expand="0" string="Group By...">
	                	<filter string="Salary Scale" icon="terp-personal" domain="[]" context="{'group_by':'grade_id'}"/>                                                           
	                </group>
           		</search>
           	</field>
        </record>
        
        <record id="view_hr_grade_line_tree" model="ir.ui.view">
            <field name="name">view.hr.grade.line.tree</field>
            <field name="model">hr.grade.line</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">	
            <tree string="Grade" editable="bottom">
            	<field name="code"/>
            	<field name="name"/>
            	<field name="grade_id" required="1" domain="[('active','=',True)]"/>
            	<field name="ratio" />
            	<field name="active"/>
           </tree>
           </field>
        </record>
        
		<record id="view_hr_grade_line_form" model="ir.ui.view">
            <field name="name">view.hr.grade.line.form</field>
            <field name="model">hr.grade.line</field>
            <field name="type">form</field>
            <field name="arch" type="xml">	
            	<form string="Grade Line">
            		<group  col="8">           	
		            	<field name="code"/>
		            	<field name="name" colspan="4"/>
		            	<field name="active"/>
		            	<field name="grade_id" required="1" domain="[('active','=',True)]"/>
		            	<field name="ratio" />
	            	</group>       	
           		</form>
           	</field>
        </record>   
	            
        <record id="action_hr_grade_line" model="ir.actions.act_window">
            <field name="name">Employee Grade</field>
            <field name="res_model">hr.grade.line</field>
            <field name="view_type">form</field>
            <field name="context">{'search_default_active_filter': 1}</field>
            <field name="view_id" ref="view_hr_grade_line_tree"/>
        </record>
        <menuitem id="menu_hr_grade_line" parent="menu_hr_grade_scale" action="action_hr_grade_line"/>
        
==============
contract duration
==============   
		<record model="ir.ui.view" id="view_hr_contract_duration_filter">
            <field name="name">hr.contract.duration.filter</field>
            <field name="model">hr.contract.duration</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
            <search string="Search Contract Duration">
            	<filter icon="terp-personal+" domain="[('active','=',True)]" name="active_filter" string="Active"/>
             	<field name="code"/>
                <field name="name"/>
                <field name="active"/>     
            </search>            
            </field>
        </record>  
                   
        <record model="ir.ui.view" id="view_hr_contract_duration_form">
            <field name="name">hr.contract.duration.form</field>
            <field name="model">hr.contract.duration</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            <form string="Contract Duration" version="7.0">
             	<group col="6">
             		<field name="code"/>
                	<field name="name" />  
                	<field name="active"/>  	
                	<field name="month"/>  
                </group>
            </form>            
            </field>
        </record>
        
        <record model="ir.ui.view" id="view_hr_contract_duration_tree">
            <field name="name">hr.contract.duration</field>
            <field name="model">hr.contract.duration</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
            <tree string="Contract Duration">
            	<field name="code"/>
                <field name="name"/>    
                <field name="active"/>
                <field name="month"/> 
            </tree>            
            </field>
        </record>
        
        <record id="action_contract_duration" model="ir.actions.act_window">
            <field name="name">Contract Duration</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hr.contract.duration</field>
            <field name="context">{'search_default_active_filter': 1}</field>
            <field name="view_type">form</field>
        </record>
        
         <menuitem 
            sequence="5"
            id="menu_hr_contract_duration"  
            action="action_contract_duration"
            parent="hr_contract.next_id_56"/>
============
reward and discipline
============
		<record id="view_hr_reward_discipline_filter" model="ir.ui.view">
            <field name="name">hr.reward.discipline.filter</field>
            <field name="model">hr.reward.discipline</field>       
            <field name="type">search</field>
            <field name="arch" type="xml">            	
            <search string="Search Reward/Discipline">                                              	 
            	<field name="employee_id"/> 
                <field name="date"/>                                         
                <group expand="0" string="Group By...">                	
                    <filter string="Employee" icon="terp-personal" domain="[]" context="{'group_by':'employee_id'}"/>
                    <filter string="Ref Document" icon="terp-personal" domain="[]" context="{'group_by':'name'}"/>                                       
                </group>	               
             </search>
             </field>
        </record>
        
		<record model="ir.ui.view" id="view_hr_reward_discipline_form">
            <field name="name">hr.reward.discipline.form</field>
            <field name="model">hr.reward.discipline</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            <form string ="Reward/Discipline" version="7.0">
            	<sheet>
            	<group>
            		<field name="name" required="1"/>
            		<field name="date" required="1"/>
            		<field name="type" required="1"/>
	            	<field name="employee_id" required="1"/>
	            	<field name="description" nolabel="1" colspan="4" placeholder="Desciption"/>	            	                
                </group>
                </sheet>  	
            </form>            
            </field>
        </record>
        
        <record model="ir.ui.view" id="view_hr_reward_discipline_tree">
            <field name="name">hr.reward.discipline.tree</field>
            <field name="model">hr.reward.discipline</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
            <tree string="Reward/Discipline">
            	<field name="name"/>
            	<field name="date"/>
           		<field name="type"/>
            	<field name="employee_id" />
            	<field name="description"/>            		
            </tree>            
            </field>
        </record>
        
        <record id="action_hr_reward_discipline" model="ir.actions.act_window">
            <field name="name">Reward/Discipline</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hr.reward.discipline</field>
            <field name="view_type">form</field>
        </record>  
            
        <menuitem 
        	sequence="4"
            id="menu_hr_reward_discipline"  
            action="action_hr_reward_discipline"
            parent="hr.menu_hr_main"/>  
            
===================
History wage
===================
		<record id="view_hr_salary_history_search" model="ir.ui.view">
            <field name="name">view.hr.salary.history.search</field>
            <field name="model">hr.salary.history</field>
            <field name="type">search</field>
            <field name="arch" type="xml">	
	            <search string="Adjustment in wage">
	            	<group>
		            	<field name="employee_id"/>
		           		<field name="contract_id" required="1"/>
		            	<field name="date"/>
		            	<!-- <field name="basic_company_id"/> -->
		            	<field name="grade_id"/>
		           		<field name="basic_wage"/>
		           		<field name="total_wage"/>
		           		<field name="trial_wage"/>
		           		<field name="reason_id"/>
		           		<field name="state"/> 
		           		<field name="notes"/>
	           		</group>
	           		<group string="Group By...">
		                <filter string="Employee" icon="terp-partner" domain="[]" context="{'group_by':'employee_id'}"/>
		                <filter string="State" icon="terp-stock_symbol-selection" domain="[]"  context="{'group_by':'state'}"/>
		                <filter string="Scale" icon="terp-gtk-select-all" domain="[]"  context="{'group_by':'scale_id'}"/>
		                <filter string="Grade" icon="terp-gtk-select-all" domain="[]"  context="{'group_by':'grade_id'}"/> 
	           		</group> 		
	            </search>
           	</field>
        </record>  
         
	    <record id="view_hr_salary_history_tree" model="ir.ui.view">
            <field name="name">view.hr.salary.history.tree</field>
            <field name="model">hr.salary.history</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">	
	            <tree version="7.0" string="Adjustment in wage" colors="blue:state == 'draft';black:state == 'done';gray:state == 'cancel'">
            		<field name="employee_id"/>
	           		<field name="contract_id"/>
	           		<field name="scale_id" invisible="1"/>
	           		<field name="grade_id" invisible="1"/>
	           		<field name="basic_company_id" invisible="1"/>
	            	<field name="date"/>
	           		<field name="basic_wage"/>
	           		<field name="incentive_wage"/>
	           		<field name="total_wage"/>
	           		<field name="trial_wage"/>
	           		<field name="reason_id"/>
	           		<field name="state"/> 
	           		<field name="notes"/>	           		
	           </tree>
           </field>
        </record> 

		<record id="view_hr_salary_history_form" model="ir.ui.view">
            <field name="name">view.hr.salary.history.form</field>
            <field name="model">hr.salary.history</field>
            <field name="type">form</field>
            <field name="arch" type="xml">	
	            <form version="7.0" string="Adjustment in wage">
	            	<header>
	            		<button name="action_confirm" states="draft" string="Confirm" class="oe_highlight" type="object"/>                             
                    	<button name="action_cancel" states="done" string="Cancel" class="oe_highlight" type="object"/>
                    	<button name="action_set_draft" states="cancel" string="Set to draft" type="object"/>	           
	                    <field name="state" widget="statusbar" statusbar_visible="draft,done,cancel"/>
                	</header>
                	<sheet string="Adjustment in wage">
		            	<group col="4">
		            		<field name="date" required="1" on_change="onchange_basic(employee_id, scale_id, grade_id, date, total_wage, False, False)"/>
		            		<newline/>
		            		<field name="employee_id" required="1" on_change="onchange_basic(employee_id, scale_id, grade_id, date, total_wage, False, False)" domain="[('active','=',True)]"/>
			           		<field name="contract_id" required="1" domain="[('employee_id','=',employee_id)]"/>
			            	<field name="scale_id" required="1" on_change="onchange_basic(employee_id, scale_id, grade_id, date, total_wage, False, False)" domain="[('active','=',True)]"/>
	           				<field name="grade_id" required="1" on_change="onchange_basic(employee_id, scale_id, grade_id, date, total_wage, False, False)" domain="[('active','=',True),('grade_id','=',scale_id)]"/>	            	
			           		<field name="basic_company_id" on_change="onchange_basic(employee_id, scale_id, grade_id, date, total_wage, False, False)" domain="[('active','=',True)]"/>
			           		<field name="basic_wage"/>
			           		<field name="incentive_wage" invisible="1"/>
			           		<field name="allowances_wage"/>
                            <field name="mobile_allowances_wage"/>
			           		<field name="total_wage" on_change="onchange_basic(employee_id, scale_id, grade_id, date, total_wage, False, False)" domain="[('active','=',True)]"/>
			           		<field name="is_trial" invisible="1"/>
			           		<field name="trial_type" on_change="onchange_trial_type(trial_type)" attrs="{'invisible':[('is_trial','=',False)]}"/> 	                            							                       	
                            <field name="trial_percent" on_change="onchange_trial_wage(total_wage, trial_percent)" attrs="{'invisible':['|',('trial_type','=','fix'), ('is_trial','=',False)]}"/>											                            
                            <field name="trial_wage" attrs="{'readonly':[('trial_type','=','percent')], 'invisible':[('is_trial','=',False)]}"/>					           		
			           		<field name="reason_id" domain="[('employee_id','=',employee_id)]"/>
			           		<field name="notes" nolabel="1" colspan="4" placeholder="Notes"/>
		            	</group>
		            </sheet>
	           </form>
           </field>
        </record>  
	        
        <record id="action_hr_salary_history" model="ir.actions.act_window">
            <field name="name">Adjustment in wage</field>
            <field name="res_model">hr.salary.history</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="menu_hr_salary_history" sequence="20" parent="hr.menu_hr_main" action="action_hr_salary_history"/>
   
====================
Contract inherit
====================            
      	<record id="hr_contract_view_form_inherit" model="ir.ui.view">
            <field name="name">hr.contract.view.form.inherit</field>
            <field name="model">hr.contract</field>
            <field name="inherit_id" ref="hr_contract.hr_contract_view_form"/>
            <field name="arch" type="xml">
	           	<xpath expr="//field[@name='wage']" position="replace">
	           		<field name="department_id" invisible="1"/>
	           		<field name="scale_id"/>
	           		<field name="grade_id"/>
	           		<field name="basic_company_id"/>
	           		<field name="basic"/>
	           		<field name="wage" invisible="1"/>  
	           		<field name="trial_wage"/>		
	           	</xpath>
	           	<xpath expr="//field[@name='wage']" position="after">
                            <field name="allowances_wage"/>
                            <field name="mobile_allowances_wage"/>
	           		<field name="total_wage"/>
	           	</xpath>
	           	<xpath expr="//group[@name='duration_group']" position="replace"> 
	           		<group name="duration_group" string="Duration">
	           			<label for="duration_type" string="Contract Duration"/>
	           			<div>
	           				<field name="duration_type" domain="[('active','=',1)]" class="oe_inline" on_change="on_change_start_date(date_start, duration_type)"/>
	           			</div>
	           			<label for="trial_date_start" string="Trial Period Duration"/>
                        <div>
                            <field name="trial_date_start" class="oe_inline"/> - <field name="trial_date_end" class="oe_inline"/>
                        </div>
                        <label for="date_start" string="Duration"/>
                        <div>
                            <field name="date_start" class="oe_inline" on_change="on_change_start_date(date_start, duration_type)"/> - <field name="date_end" class="oe_inline"/>
                        </div>
                        <field name="working_hours" invisible="1"/>
                        <field name="salary_type"/>
	           		</group>                 
	        	</xpath>
	        	<xpath expr="//group[@name='duration_group']" position="after">
	        		<group string="Version">
	        			<field name="reversion" required="1"/>
	        			<field name="date_reversion"/>
	        		</group>
	        	</xpath> 
	        	
	        	<xpath expr="//page[@string='Work Permit']" position="before">
	        		<page string="Wage History">
	        			<field name="history">
		        			<tree string="Adjustment in wage" colors="blue:state == 'draft';black:state == 'done';gray:state == 'cancel'">	            				
	            				<field name="employee_id" invisible="1"/>
				           		<field name="contract_id" invisible="1"/>
				            	<field name="date"/>				            	
				           		<field name="basic_wage"/>
				           		<field name="incentive_wage" invisible="1"/>
				           		<field name="allowances_wage"/>
                                                 <field name="mobile_allowances_wage"/>
				           		<field name="total_wage"/>
				           		<field name="trial_wage"/>
				           		<field name="notes"/>
				           		<field name="state"/> 
				           		<button name="action_confirm" icon="terp-camera_test" states="draft" string="Confirm" class="oe_highlight" type="object"/>                             
			                    <button name="action_cancel"  icon="gtk-cancel" states="done" string="Cancel" class="oe_highlight" type="object"/>
			                    <button name="action_set_draft" icon="gtk-convert" states="cancel" string="Set to draft" type="object"/>
				           						           			           
				           </tree>
		        			<form string="Adjustment in wage">
				            	<header>
				            		<button name="action_confirm" states="draft" string="Confirm" class="oe_highlight" type="object"/>                             
			                    	<button name="action_cancel" states="done" string="Cancel" class="oe_highlight" type="object"/>
			                    	<button name="action_set_draft" states="cancel" string="Set to draft" type="object"/>	           
				                    <field name="state" widget="statusbar" statusbar_visible="draft,done,cancel"/>
			                	</header>
			                	<newline/>			                			
				            	<group col='4'>
				            		<field name="date" required="1" on_change="onchange_basic(employee_id, scale_id, grade_id, date, total_wage, parent.trial_date_start, parent.trial_date_end)"/>		            				
				            		<newline/>
				            		<field name="employee_id" invisible="1"/>
			           				<field name="contract_id" invisible="1"/>				           							            	
					            	<field name="scale_id" required="1" on_change="onchange_basic(parent.employee_id, scale_id, grade_id, date, total_wage, parent.trial_date_start, parent.trial_date_end)" domain="[('active','=',True)]"/>
			           				<field name="grade_id" required="1" on_change="onchange_basic(parent.employee_id, scale_id, grade_id, date, total_wage, parent.trial_date_start, parent.trial_date_end)" domain="[('active','=',True),('grade_id','=',scale_id)]"/>	            	
					           		<field name="basic_company_id" on_change="onchange_basic(parent.employee_id, scale_id, grade_id, date, total_wage, parent.trial_date_start, parent.trial_date_end)" domain="[('active','=',True)]"/>
					           		<field name="basic_wage"/>
					           		<field name="incentive_wage" invisible="1"/>
					           		<field name="allowances_wage"/>
                                                        <field name="mobile_allowances_wage"/>
					           		<field name="total_wage" on_change="onchange_basic(parent.employee_id, scale_id, grade_id, date, total_wage, parent.trial_date_start, parent.trial_date_end)" domain="[('active','=',True)]"/>
					           		<field name="is_trial" invisible="1"/>
					           		<field name="trial_type" on_change="onchange_trial_type(trial_type)" attrs="{'invisible':[('is_trial','=',False)]}"/> 	                            							                       	
		                            <field name="trial_percent" on_change="onchange_trial_wage(total_wage, trial_percent)" attrs="{'invisible':['|',('trial_type','=','fix'), ('is_trial','=',False)]}"/>											                            
		                            <field name="trial_wage" attrs="{'readonly':[('trial_type','=','percent')], 'invisible':[('is_trial','=',False)]}"/>
					           		<field name="reason_id" domain="[('employee_id','=',employee_id)]"/>
					           		<field name="notes" nolabel="1" colspan="4" placeholder="Notes"/>
				            	</group>  
				           </form>
	        			</field>
	        		</page>
	        	</xpath>	
            </field>
        </record>
        
        <record id="hr_contract_view_tree" model="ir.ui.view">
            <field name="name">hr.contract.view.tree</field>
            <field name="model">hr.contract</field>       
            <field name="inherit_id" ref="hr_contract.hr_contract_view_tree"/>
            <field name="arch" type="xml">
            	<tree string="Contracts" position="replace">
	                <tree colors="red:status_contract_end==2;blue:status_contract_end==0;Goldenrod:status_contract_end==1" string="Contracts">
	                    <field name="department_id" invisible="1"/>
	                    <field name="name"/>
	                    <field name="employee_id"/>
	                    <field name="job_id" widget="selection"/>         
	                    <field name="date_start"/>
	                    <field name="date_end"/>
	                    <field name="duration_type" invisible="1"/>
	                    <field name="salary_type" invisible="1"/>
	                    <field name="reversion" required = "1"/> 
	                    <field name="date_reversion"/>
                           <field name="status_contract_end" invisible="1"/>     	         
	                </tree>
                </tree>
            </field>
        </record>          
        <record id="hr_contract_view_search" model="ir.ui.view">
            <field name="name">hr.contract.view.search</field>
            <field name="model">hr.contract</field>
            <field name="inherit_id" ref="hr_contract.hr_contract_view_search"/>
            <field name="arch" type="xml">
            	<filter string="Contract Type" position="replace">
            		<filter string="Duration Type" icon="terp-stock_symbol-selection" domain="[]" context="{'group_by':'duration_type'}"/>
            		<filter string="Salary Type" icon="terp-stock_symbol-selection" domain="[]" context="{'group_by':'salary_type'}"/>
            	</filter>
            	<filter string="Working Schedule" position="replace"></filter>
            	<filter string="Employee" position="before">
            		<filter string="Department" icon="terp-personal" domain="[]" context="{'group_by':'department_id'}"/>
            	</filter>              
            </field>
        </record>
    </data>      
</openerp>
