		<!-- Maternity Tree View -->
		    <record model="ir.ui.view" id="hr_maternity_tree">
		        <field name="name">hr.maternity.tree</field>
		        <field name="model">hr.maternity</field>
		        <field name="type">tree</field>
		        <field name="arch" type="xml">
		            <tree string="Maternity">		     	
		                <field name="code"/>
