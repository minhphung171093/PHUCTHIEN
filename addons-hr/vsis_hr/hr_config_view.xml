		            </tree>
		        </field>
		    </record>
		
		<!-- Maternity Form View -->
		    <record model="ir.ui.view" id="hr_maternity_form">
		        <field name="name">hr.maternity.form</field>
		        <field name="model">hr.maternity</field>
		        <field name="type">form</field>
		        <field name="arch" type="xml">
		            <form string="Maternity" version="7.0">		
		            	<group col="6">     	
			                <field name="code"/>
			                <field name="name"/>
			                <field name="active"/>
			                <label for="date_start" string="Duration"/>
			                <div>
				                <field name="date_start" class="oe_inline"/> - 
				                <field name="date_stop" class="oe_inline"/>
			                </div>			                
			                <field name="month"/>
