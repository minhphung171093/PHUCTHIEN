		        <field name="view_type">form</field>
		        <field name="context">{'search_default_active_filter': 1}</field>
		        <field name="view_id" ref="hr_maternity_tree"/>
		    </record>
	    
	    	<menuitem
	            id="hr_maternity_menu"
	            name="Maternity"
	            parent="menu_hr_configuration_employee"
	            action="hr_maternity_action"
